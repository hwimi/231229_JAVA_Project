package day12;

/*-학생정보를 관리하기 위한 클래스
 * -학생 기본정보 : 이름, 생년월일, 전화번호 나이
 * -학원정보:학원명="EZEN" final static, 지점
 * -수강정보:수강과목, 기간
 * 여러과목을 들을수 있음 (여러과목을 수강하기 위해서는 배열로 처리 5과목까지 가능)
 * ex) 홍길동 0001010 010-1111-111 25
 * EZEN(인천)
 * 자바6개월 파이썬1개월 빅데이터 3개월
 * 기능(메서드)
 * -학생기본정보를 출력하능 기능
 * -학원정보를 출력하는기능
 * -수강정보를 출력하는 기능
 * -학생의 수강 정보를 추가하는 기능
 */
public class 과제 {

	public static void main(String[] args) {
		new student("acdamy", "이휘근", "19990618", "01050642820", "25", "자바", "인천");
		System.out.println(student);
		
		
		
		student subject[]=new student[5];
		for(int i=0; i<subject.length; i++) {
			
		}
		
		
		
		

	}

}

class student{
	public final static String acdamy="EZEN";
	public String name;
	public String birth_date;
	public String number;
	public String age;
	public String gigum;
	public String subject;
	
	public student(){
		
	}
	public student(String acdamy,String name,String birth_date,String number,String age,String subject, String gigum) {
		this.name=name;
		this.birth_date=birth_date;
		this.number=number;
		this.age=age;
		this.gigum=gigum;
		this.subject=subject;
		
	}
	public static void printacdamy() {
		System.out.println("acdamy:"+acdamy);
	}
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getBirth_date() {
		return birth_date;
	}
	public void setBirth_date(String birth_date) {
		this.birth_date = birth_date;
	}
	public String getNumber() {
		return number;
	}
	public void setNumber(String number) {
		this.number = number;
	}
	public String getAge() {
		return age;
	}
	public void setAge(String age) {
		this.age = age;
	}
	public String getGigum() {
		return gigum;
	}
	public void setGigum(String gigum) {
		this.gigum = gigum;
	}
	public String getSubject() {
		return subject;
	}
	public void setSubject(String subject) {
		this.subject = subject;
	}
	public static String getAcdamy() {
		return acdamy;
	}
	@Override
	public String toString() {
		return acdamy+":"+" [name=" + name + ", birth_date=" + birth_date + ", number=" + number + ", age=" + age
				+ ", gigum=" + gigum + ", subject=" + subject + "]";
	}
	
	
	
	
	

	
}

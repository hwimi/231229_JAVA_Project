package day11;

public class 과제 {

	public static void main(String[] args) {
		/*메서드를 이용하여 작업
		 * 두정수의 값이 주어졌을때 두 정수의 사이의 모든 정수의 합을 리턴하는 메서드
		 * ex)a=3,b=5=> 3+4+5
		 * ex)a=5,b=1=>1+2+3+4+5
		 * ex)a=3 b=3=>3
		 */
		과제 a=new 과제();
		System.out.println(a.sum(3, 3));

	}

	public int sum(int num1,int num2) {
		if(num1==num2) {
			return num1;
		}
		int result=0;
		int a=(int)(Math.random())+1;
		if(a>num1||num2<a) {
			result=num1+num2;
		}
		return result;
		
		
		
		
		
	}
	
	}


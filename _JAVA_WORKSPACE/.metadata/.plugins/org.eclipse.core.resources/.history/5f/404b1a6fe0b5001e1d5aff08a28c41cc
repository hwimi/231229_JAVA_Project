package day11;

public class Ex {

	public static void main(String[] args) {
		CardPack cp=new CardPack();
		int cnt=0;
		for(int i=0; i<4; i++) {
			for(int j=0; j<13; j++) {
				cp.getPack()[cnt].pick();
				//cp.getPack:CardPack 클래스 안에있는 pack[]
				//cp.getPack()[cnt] :pack[0] =>card(멤버변수 메서드)
				cnt++;
				}
			System.out.println();
				
			}
		}
		

	}


